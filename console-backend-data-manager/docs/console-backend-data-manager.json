			}
		},
		"/api/dm/datasets/{datasetId}": {
			"get": {
				"tags": [
					"datasets"
				],
				"summary": "Get info for a dataset",
				"parameters": [
					{
						"name": "datasetId",
						"in": "path",
						"description": "dataset ID",
						"required": true,
						"type": "string"
					}
				],
				"produces": [
					"application/xml"
				]
			},
			"put": {
				"tags": [
					"datasets"
				],
				"summary": "Change data retention policies for a dataset",
				"parameters": [
					{
						"name": "datasetId",
						"in": "path",
						"description": "dataset ID",
						"required": true,
						"type": "string"
					},
					{
						"in": "body",
						"name": "body",
						"description": "--",
						"required": true,
						"type": "object",
						"schema": {
							"properties": {
								"mode": {
									"name": "mode",
									"required": false,
									"type": "string",
									"enum": ["archive", "delete"]
								},
								"policy": {
									"name": "mode",
									"required": false,
									"type": "string",
									"enum": ["age", "size"]
								},
								"max_age_days": {
									"name": "mode",
									"required": false,
									"type": "integer",
									"format": "int64",
									"minimum": 1,
									"maximum": 36500
								},
								"max_size_gigabytes": {
									"name": "mode",
									"required": false,
									"type": "integer",
									"format": "int64",
									"minimum": 1,
									"maximum": 1000000
								}
					        },
					        "type": "object"
						}
					}
				],
				"consumes": [
					"application/json"
				],
				"produces": [
					"application/xml"
				]
			}
		},
